<?xml version="1.0"?>
<openerp>
<data noupdate="0">

    #
    # Sequences for Asset items
    #

   
  
    
    <record model="ir.sequence.type" id="seq_type_issue_tracker">
        <field name="name">Asset Transfer</field>
        <field name="code">asset.transfer</field>
    </record>

    <record model="ir.sequence" id="seq_issue_tracker">
        <field name="name">Asset Transfer</field>
        <field name="code">asset.transfer</field>
        <field name="prefix">TF/%(year)s/%(month)s/%(day)s/</field>
        <field name="padding">2</field>
        <field name="company_id" eval="False" />
    </record>
    
      <record model="ir.sequence.type" id="seq_type_asset_disposal_tracker">
        <field name="name">OD Asset Disposal</field>
        <field name="code">od.asset.disposal</field>
    </record>

    <record model="ir.sequence" id="seq_asset_disposal_tracker">
        <field name="name">OD Asset Disposal</field>
        <field name="code">od.asset.disposal</field>
        <field name="prefix">ADP/%(year)s/%(month)s/%(day)s/</field>
        <field name="padding">2</field>
        <field name="company_id" eval="False" />
    </record>

</data>
</openerp>
