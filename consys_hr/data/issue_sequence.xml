<?xml version="1.0"?>
<openerp>
<data noupdate="0">

    #
    # Sequences for Issue tracker
    #

   
    <record model="ir.sequence.type" id="seq_type_issue_tracker">
        <field name="name">Issue Tracker</field>
        <field name="code">issue.tracker</field>
    </record>

    <record model="ir.sequence" id="seq_issue_tracker">
        <field name="name">Issue Tracker</field>
        <field name="code">issue.tracker</field>
        <field name="prefix">IS/%(year)s/%(month)s/%(day)s/</field>
        <field name="padding">2</field>
        <field name="company_id" eval="False" />
    </record>

</data>
